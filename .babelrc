{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@components": "./components",
          "@utils": "./utils",
          "@config": "./config",
          "@pages": "./pages",
          "@services": "./services",
          "@contexts": "./contexts",
          "@styles": "./styles"
        }
      }
    ]
  ]
}
