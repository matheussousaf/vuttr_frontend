{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@components": "./src/components",
          "@utils": "./src/utils",
          "@config": "./src/config",
          "@pages": "./src/pages",
          "@services": "./src/services",
          "@contexts": "./src/contexts",
          "@styles": "./src/styles",
          "@tests": "./src/tests",
          "@interfaces": "./src/interfaces"
        }
      }
    ]
  ]
}
